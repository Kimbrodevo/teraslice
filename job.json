{
  "name": "Reindex Events",
  "lifecycle": "once",
  "analytics": false,
  "operations": [
    {
      "_op": "elasticsearch_reader",
      "index": "events-*",
      "size": 5000,
      "start": "2015-10-08T19:38:49",
      "end": "2015-10-09",
      "interval": "5_min",
      "date_field_name": "created",
      "full_response": true
    },
    {
      "_op": "elasticsearch_index_selector",
      "index": "bigdata3",
      "type": "events"
    },
    {
      "_op": "elasticsearch_bulk_insert",
      "size": 5000
    }
  ]
}