{
  "name": "Reindex Events",
  "lifecycle": "once",
  "workers": 5,
  "analytics": false,
  "operations": [
    {
      "_op": "elasticsearch_reader",
      "index": "events-*",
      "size": 5000,
      "start": " 2015-10-14T03:03:05",
      "end": " 2015-10-14T03:03:07",
      "interval": "1_s",
      "date_field_name": "created"
    },
    {
      "_op": "elasticsearch_index_selector",
      "index": "bigdata123",
      "type": "events"
    },
    {
      "_op": "elasticsearch_bulk_insert",
      "size": 5000
    }
  ]
}