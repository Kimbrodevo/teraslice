{
  "name": "Reindex Events",
  "lifecycle": "once",
  "analytics": false,
  "operations": [
    {
      "_op": "elasticsearch_reader",
      "index": "events-*",
      "size": 5000,
      "start": "2015-08-25",
      "end": "2015-08-26",
      "interval": "5_mins",
      "date_field_name": "@timestamp"
    },
    {
      "_op": "elasticsearch_index_selector",
      "index": "bigdata43",
      "type": "events"
    },
    {
      "_op": "elasticsearch_bulk_insert",
      "size": 5000
    }
  ]
}
