

{
  "name": "Reindex Events",
  "lifecycle": "once",
  "analytics": false,
  "operations": [
    {
      "op": "elasticsearch_reader",
      "index": "events-*",
      "size": 5000,
      "auth": "someToken",
      "start": "2015-07-08T00:00:00",
      "end": "2015-07-08T00:10:00",
      "interval": "5_mins",
      "dateFieldName": "@timestamp",
      "filter": ""

    },
    {
      "op": "elasticsearch_index_selector",
      "index": "bigdata20",
      "type": "events",
      "preserve_id": true
    },
    {
      "op": "elasticsearch_bulk_insert",
      "size": 5000
    }
  ]
}
