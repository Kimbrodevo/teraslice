{
  "name": "Reindex Events",
  "lifecycle": "persistent",
  "analytics": false,
  "operations": [
    {
      "_op": "elasticsearch_reader",
      "index": "bigdata5",
      "size": 5000,
      "start": "00_s",
      "end": "59_s",
      "date_field_name": "created",
      "full_response": true
    },
    {
      "_op": "elasticsearch_index_selector",
      "index": "bigdata6",
      "type": "events"
    },
    {
      "_op": "elasticsearch_bulk_insert",
      "size": 5000
    }
  ]
}
