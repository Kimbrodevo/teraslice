{
  "name": "Reindex Events",
  "lifecycle": "once",
  "analytics": false,
  "operations": [
    {
      "op": "elasticsearch_reader",
      "index": "events-*",
      "size": 5000,
      "start": "2015-08-25",
      "end": "2015-08-26",
      "interval": "5_mins",
      "dateFieldName": "@timestamp"
    },
    {
      "op": "elasticsearch_index_selector",
      "index": "bigdata10",
      "type": "events"
    },
    {
      "op": "elasticsearch_bulk_insert",
      "size": 5000
    }
  ]
}
